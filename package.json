{
	"name": "inconnu-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node -r dotenv/config build/index.js",
		"prepare": "svelte-kit sync || echo ''",
