		"vite": "^6.0.0"
	},
	"dependencies": {
		"@auth/sveltekit": "^1.11.1",
		"@lucide/svelte": "^0.479.0",
		"dotenv": "^17.2.4",
		"isomorphic-dompurify": "^2.22.0",
		"marked": "^15.0.7"
	}
